<view><block wx:if="{{loginStatus}}"><card vue-id="1" headTitle="账号管理" bodyStyle="background: #F5F5F5;" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{userinfo}}" wx:for-item="item" wx:for-index="index" wx:key="index"><uni-list-item vue-id="{{('2-'+index)+','+('1')}}" title="{{item.title}}" data-event-opts="{{[['^click',[['navigeto',['$0'],[[['userinfo','',index,'path']]]]]]]}}" bind:click="__e" bind:__l="__l"></uni-list-item></block></card></block><card vue-id="3" headTitle="关于" bodyStyle="background: #F5F5F5;" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{list}}" wx:for-item="list" wx:for-index="listindex" wx:key="listindex"><uni-list-item vue-id="{{('4-'+listindex)+','+('3')}}" title="{{list.title}}" data-event-opts="{{[['^click',[['navigeto',['$0'],[[['list','',listindex,'path']]]]]]]}}" bind:click="__e" bind:__l="__l"></uni-list-item></block></card><view class="p-3"><button class="bg-white" type="default" data-event-opts="{{[['tap',[['logoutEvent']]]]}}" bindtap="__e">退出登录</button></view></view>